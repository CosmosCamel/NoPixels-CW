{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "admin_address",
    "cooldown",
    "height",
    "width"
  ],
  "properties": {
    "admin_address": {
      "type": "string"
    },
    "cooldown": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "end_height": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "start_height": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "collection_address": {
      "type": [
        "string",
        "null"
      ],
    },
    "height": {
      "description": "How many chunks high",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "width": {
      "description": "How many chunks wide",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
